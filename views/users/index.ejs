<%- include('../partials/boilerplate') %>
<body>
    <h1>Users</h1>
    <% for (let i=0; i<users.length; i++) { %>
        <img src='<%=users[i].img%>' alt="User">
        <a href='/users/profile/<%=users[i].uuid%>'><%= users[i].username %></a>
        <p><%= users[i].aboutMe %></p>

    <% } %>
</body>
</html>